{
    "family": "env-autonomic",
    "taskRoleArn": "",
    "executionRoleArn": "arn:aws:iam::account_number:role/ecsTaskExecutionRole",
   
    "containerDefinitions": [
        {
            "name": "ecs-env-autonomic",
            "image": "account_number.dkr.ecr.region.amazonaws.com/env-autonomicapi:tag",
           
           
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "ASPNETCORE_ENVIRONMENT",
                    "value": "interneuron_environment"
                }
            ]
          
        }
    ],

    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "500",
    "memory": "1024",
    "pidMode": "task",
    "ipcMode": "task"
}
